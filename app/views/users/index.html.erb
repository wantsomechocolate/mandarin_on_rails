<section class="wrapper style1 align-left input-text-index-section">
	<div class="inner">

		<table>
			<tr>
				<th>Email</th>
				<th>Approved</th>
				<th>Is Guest</th>
				<th>View</th>
				<th>Edit</th>
			</tr>

		  <% @users.each do |user| %>
		    <tr>
		      <td><%= user.email %> </td>
		      <td><%= user.approved %> </td>
		      <td><%= user.guest %> </td>
		      <td><%= link_to "View", user_path(user) %> </td>
		      <td><%= link_to "Edit", edit_user_path(user) %> </td>
		    </tr>
		  <% end %>
		</table>

	</div>
</section>
