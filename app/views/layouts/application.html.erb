<html>
  <head>
    <title>Segmentor</title>
    <%= favicon_link_tag(source = "/favicon.ico") %>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <noscript><%= stylesheet_link_tag "noscript", "data-turbo-track": "reload" %></noscript>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.12.1/af-2.4.0/b-2.2.3/b-colvis-2.2.3/b-html5-2.2.3/b-print-2.2.3/cr-1.5.6/date-1.1.2/fc-4.1.0/fh-3.2.4/kt-2.7.0/r-2.3.0/rg-1.2.0/rr-1.2.8/sc-2.0.7/sb-1.3.4/sp-2.0.2/sl-1.4.0/sr-1.1.1/datatables.min.css"/>  
    <%= javascript_importmap_tags %>
  </head>

  <body> <!-- Taking off class="is-preload" solved all my problems. I guess the template really depended on it, but turbo I guess was messing with when body was supposed to have that tag or not. -->
    <!-- Wrapper -->
      <div id="wrapper" class="divided">

        <!-- Header -->
          <header id="header" class="alt">
            <span>&nbsp</span><span>Mandarin on Rails</span>
            <nav>
              <a href="#menu"></a>
            </nav>
          </header>

        <!-- Menu added from another template-->
          <nav id="menu">
            <% if user_signed_in? %>
              <p> Logged in as <br> <strong><%= current_user.email %></strong> <p>
            <% end %>
            <ul class="links">
              <li><a href="/">Home</a></li>
              <li><a href="/how_to_use">How to Use</a></li>
              <li><a href="/resources">Resources</a></li>
              <li><a href="/how_it_works">About</a></li>
              <!-- <li><a href="/settings">Settings</a></li> -->
            </ul>
            <ul class="actions stacked">

                  <% if user_signed_in? %>
                    <li> <%= link_to 'Profile', edit_user_registration_path, :class => 'button fit primary' %> </li>
                    <li> <%= link_to "Logout", destroy_user_session_path, :method => :delete, :class => 'button fit primary'  %> </li>
                  <% else %>
                    <li> <%= link_to "Register", new_user_registration_path, :class => 'button fit primary'  %> </li>
                    <li> <%= link_to "Login", new_user_session_path, :class => 'button fit primary'  %> </li>
                  <% end %>

            </ul>
          </nav>

          <% flash.each do |key, msg| %>
              <div class="alert alert-<%= key %>">
                  <a class="close" data-dismiss="alert">x</a>
                  <%= msg.html_safe %>
              </div>
          <% end %>
          
          <!-- <section>
            <div class="inner"> -->
              <!-- Each page is wrapped in it's own section and inner tags, I did it this way so different pages could have different classes -->
              <%= yield %>
            <!-- </div>
          </section> -->

      </div>

      <div id="wrapper2" class="divided">

        <!-- Footer -->
          <footer class="wrapper style1 align-center">
            <div class="inner">
              <ul class="icons">
                <li><a target = "_blank" href="https://github.com/wantsomechocolate" class="icon brands style2 fa-github"><span class="label">Github</span></a></li>
                <!--<li><a target = "_blank" href="#" class="icon brands style2 fa-facebook-f"><span class="label">Facebook</span></a></li>
                <li><a target = "_blank" href="#" class="icon brands style2 fa-instagram"><span class="label">Instagram</span></a></li> -->
                <li><a target = "_blank" href="https://wantsomechocolate.github.io/" class="icon style2 fa-user"><span class="label">Email</span></a></li>
                <li><a target = "_blank" href="https://www.linkedin.com/in/james-mcglynn/" class="icon brands style2 fa-linkedin-in"><span class="label">LinkedIn</span></a></li>
                
              </ul>
              <!-- <p>&copy; Untitled. Design: <a href="https://html5up.net">HTML5 UP</a>.</p> -->
            </div>
          </footer>

      </div>

      <!-- Scripts -->
      <%= javascript_include_tag "jquery.min", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "jquery.scrolly.min", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "jquery.scrollex.min", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "browser.min", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "breakpoints.min", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "util", "data-turbo-track" => "reload"  %>
      <%= javascript_include_tag "main", "data-turbo-track" => "reload"  %>

      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
      <script type="text/javascript" src="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.12.1/af-2.4.0/b-2.2.3/b-colvis-2.2.3/b-html5-2.2.3/b-print-2.2.3/cr-1.5.6/date-1.1.2/fc-4.1.0/fh-3.2.4/kt-2.7.0/r-2.3.0/rg-1.2.0/rr-1.2.8/sc-2.0.7/sb-1.3.4/sp-2.0.2/sl-1.4.0/sr-1.1.1/datatables.min.js"></script>

      <%= javascript_include_tag "custom", "data-turbo-track" => "reload"  %>

  </body>
</html>